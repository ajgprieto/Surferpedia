# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()
GET     /page1                           controllers.Application.page1()
GET     /koloheandino                      controllers.Application.koloheandino()
GET     /keliamoniz                     controllers.Application.keliamoniz()
GET     /noa                         controllers.Application.noa()
GET     /laird                         controllers.Application.laird()
GET     /kalanid                        controllers.Application.kalanid()
GET     /malia                      controllers.Application.malia()


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
